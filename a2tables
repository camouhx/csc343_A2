CREATE TABLE Query1( 
	fname VARCHAR(20) NOT NULL,
	lname VARCHAR(20) NOT NULL,
	country VARCHAR(20) NOT NULL);

CREATE TABLE Query2(
	name VARCHAR(20) NOT NULL,
	num INTEGER);

CREATE TABLE Query3(
	isOpeningGame BOOLEAN);

CREATE TABLE Query4(
	mid INTEGER);

CREATE TABLE Query5(
	name VARCHAR(20) NOT NULL);

CREATE TABLE Query6(
	pid INTEGER,
	fname VARCHAR(20) NOT NULL,
	lname VARCHAR(20) NOT NULL,
	minutes INTEGER);

CREATE TABLE Query7(
	name VARCHAR(20) NOT NULL,
	coach VARCHAR(20) NOT NULL,
	budget INTEGER);
